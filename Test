-- FLUENT UI
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Script Tổng Hợp",
    SubTitle = "By White",
    TabWidth = 160,
    Size = UDim2.fromOffset(530, 350), -- 480,360 or 530, 350
    Acrylic = false, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark", -- Rose, Dark, Light, Darker, Aqua
    MinimizeKey = Enum.KeyCode.End -- Used when theres no MinimizeKeybind
})
-- TABS
local Tabs = {
    A = Window:AddTab({ Title = "Information", Icon = "" }),
    B = Window:AddTab({ Title = "Blox Fruit", Icon = "" }),
    C = Window:AddTab({ Title = "Arise Crossover", Icon = "" }),
    D = Window:AddTab({ Title = "Dead Rails", Icon = "" }),
    E = Window:AddTab({ Title = "Blue Lock: Rivals", Icon = "" }),
    G = Window:AddTab({ Title = "Tsb", Icon = "" }),
    H = Window:AddTab({ Title = "MM2", Icon = "" }),
    I = Window:AddTab({ Title = "Anime Vanguards", Icon = "" }),
    J = Window:AddTab({ Title = "Pets Sim 99", Icon = "" }),
    K = Window:AddTab({ Title = "Pets Go", Icon = "" }),
    L = Window:AddTab({ Title = "Blade Ball", Icon = "" }),
    Q = Window:AddTab({ Title = "Fisch", Icon = "" }),
    M = Window:AddTab({ Title = "Zam2109's Hub", Icon = "" }),
    N = Window:AddTab({ Title = "Haha Gamer Hub", Icon = "" }),
    O = Window:AddTab({ Title = "Another Scripts", Icon = "" }),
    P = Window:AddTab({ Title = "Misc", Icon = "" }),
}
